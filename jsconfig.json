{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "baseUrl": "./",
    "paths": {
      "assets/*": ["./src/assets/*"],
      "components/*": ["./src/components/*"],
      "constants/*": ["./src/constants/*"],
      "features/*": ["./src/assets/*"],
      "src/*": ["./src/*"],
      "utils/*": ["./src/utils/*"]
    }
  },
  "exclude": ["node_modules", "coverage", "out", "dist", "public", "nginx", ".vscode"]
}
